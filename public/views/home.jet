<style>
    .animation {
        position: relative;
        width: 100%;
        height: 100%;
        }

        .sentence {
        position: relative;
        width: 100%;
        height: 100%;
        text-align: center;
        font-size: 72px;
        animation: change-sentence 10s infinite;
        }

        .sentence:before {
        content: "e Do Branding";
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        text-align: center;
        font-size: 72px;
        animation: change-sentence 10s infinite;
        }

        .sentence:nth-of-type(1):before {
        animation-delay: 3s;
        }
        .sentence:nth-of-type(2):before {
        content: "esign Brands";
        animation-delay: 6s;
        }
        .sentence:nth-of-type(3):before {
        content: "evelop Brands";
        animation-delay: 9s;
        }

        @keyframes change-sentence {
        0% {
            opacity: 0;
        }
        33.33% {
            opacity: 1;
        }
        66.66% {
            opacity: 1;
        }
        100% {
            opacity: 0;
        }
        }


</style>
    

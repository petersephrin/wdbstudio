  <script>
    
    var className = "inverted";
    var scrollTrigger = 60;
    var main = document.getElementById("main");
    main.onscroll = function() {
      if (main.scrollTop >= scrollTrigger) {
        document.getElementById("header").classList.add(className);
      } else {
        document.getElementById("header").classList.remove(className);
      }
    };

    var currentActiveLink = document.getElementById("homelink");
    var featureslink = document.getElementById("featureslink");
    


    function activateLink(element){
      currentActiveLink.classList.remove("activelink");
      element.classList.add("activelink");
      currentActiveLink = element;
    }

  </script>


